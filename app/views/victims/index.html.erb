<p id="notice"><%= notice %></p>

<h1>Victims</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Real email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @victims.each do |victim| %>
      <tr>
        <td><%= link_to victim.name, victim %></td>
        <td><%= mail_to victim.real_email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
