<p id="notice"><%= notice %></p>

<h1>Fake Emails</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fake_emails.each do |fake_email| %>
      <tr>
        <td><%= link_to fake_email.email, fake_email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fake Email', new_fake_email_path %>
